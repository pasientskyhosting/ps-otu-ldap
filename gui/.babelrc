{
    "env": {
        "production": {
            "presets": ["@babel/preset-env", "@babel/preset-typescript", "@babel/preset-react"],
            "plugins": [
                "@babel/transform-runtime"
            ]
        },
        "development": {
            "presets": ["@babel/preset-env", "@babel/preset-typescript", "@babel/preset-react"],
            "plugins": [
                "react-hot-loader/babel",
                "@babel/transform-runtime"
            ]
        }
    }
}
